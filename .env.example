# PostgreSQL connection string used by Prisma.
# Must start with postgresql:// or postgres://
DATABASE_URL="postgresql://USER:PASSWORD@HOST:5432/DBNAME?sslmode=require"

# Secret used to authorize scheduler calls to /api/jobs/send-quotes.
CRON_SECRET="replace-with-a-long-random-secret"

# Secret used to encrypt stored Vestaboard credentials.
ENCRYPTION_KEY="replace-with-a-long-random-secret"

# Reserved for future app authentication.
APP_SINGLE_USER_SECRET="replace-with-a-long-random-secret"
